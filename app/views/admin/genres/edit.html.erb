<div class="container">
  <h3 class="col-lg-3 my-4" , style="background-color:#d7d7d7">ジャンル編集</h3>
  <div class="row">
    <div class="col  m-5">
    <%= form_with model: @genre, url: admin_genre_path(@genre) do |f| %>
　  <div class="form-group">
　   <strong><%= f.label :name, 'ジャンル名' %></strong>
     <%= f.text_field :name %>
　　 <%= f.submit '変更を保存',class:"btn btn-success" %>
　  </div>
　  <% end %>
　  </div>
  </div>
</div>
